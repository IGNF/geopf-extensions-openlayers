{
  "name": "geoportal-extensions-openlayers",
  "description": "French Geoportal Extensions for OpenLayers libraries",
  "version": "3.0.0",
  "date": "01/02/2024",
  "main": "dist/packages/GpPluginOpenLayers.js",
  "types": "dist/packages/index.d.ts",
  "module": "src/packages/index.js",
  "directories": {},
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "setup": "npm install",
    "clean": "echo \"Warning: target not yet implemented!\" && exit 0",
    "eslint": "eslint src/",
    "analyze": "npm run build:src -- --env samples=no --env jsdoc=no --env linter=no --analyze",
    "test": "npm run test:serve",
    "test:serve": "echo \"Warning: target not yet implemented!\" && exit 0",
    "sample": "npm run sample:serve -- --env jsdoc=no",
    "sample:serve": "webpack serve --config build/webpack/webpack.config.modules --static-directory . --mode development --open samples/index-modules-map.html",
    "generate-types": "npx tsc -p build/types/tsconfig.json",
    "generate-jsdoc": "jsdoc -c build/jsdoc/jsdoc.json",
    "doc": "npm run doc:serve -- --env samples=no --env linter=no",
    "doc:serve": "webpack serve --config build/webpack/webpack.config.modules --static-directory jsdoc/ --open",
    "build": "npm-run-all --print-label --print-name build:*",
    "build:dev": "webpack --config build/webpack/webpack.config.modules --mode=development",
    "build:prod": "webpack --config build/webpack/webpack.config.modules --mode=production",
    "build:src": "webpack --config build/webpack/webpack.config.modules --mode=none"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IGNF/geoportal-extensions-openlayers.git"
  },
  "author": "IGNF",
  "keywords": [
    "geoportail",
    "geoplateforme",
    "javascript",
    "OpenLayers 8"
  ],
  "license": "CECILL-B",
  "bugs": {
    "url": "https://github.com/IGNF/geoportal-extensions-openlayers/issues"
  },
  "homepage": "https://github.com/IGNF/geoportal-extensions-openlayers#readme",
  "dependencies": {
    "@gouvfr/dsfr": "^1.11.0",
    "@mapbox/mapbox-gl-style-spec": "14.1.0",
    "eventbusjs": "0.2.0",
    "geoportal-access-lib": "3.4.0",
    "loglevel": "^1.9.1",
    "ol": "8.2.0",
    "ol-mapbox-style": "12.2.0",
    "proj4": "2.10.0",
    "sortablejs": "1.15.2",
    "typescript": "^5.3.3",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-template-literals": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.35.1",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-webpack-plugin": "^4.0.1",
    "exports-loader": "^5.0.0",
    "expose-loader": "^5.0.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "handlebars-layouts": "^3.1.4",
    "html-webpack-plugin": "^5.6.0",
    "jsdoc-webpack-plugin": "^0.3.0",
    "mini-css-extract-plugin": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "responsive-loader": "^3.1.2",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "string-replace-loader": "^3.1.0",
    "string-template": "^1.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  }
}
